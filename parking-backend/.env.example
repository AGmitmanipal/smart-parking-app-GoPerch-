# Express/Mongoose backend environment variables
# Copy to .env and fill in values.

NODE_ENV=development

# MongoDB connection string
MONGODB_URI=mongodb://127.0.0.1:27017/parkingappDB

# Optional: restrict CORS to your frontend origin(s). Comma-separated.
CORS_ORIGIN=http://localhost:5173

# Ports (for local dev when running BOTH servers). In production, your platform may set PORT.
ZONES_PORT=5000
RESERVATIONS_PORT=7000
